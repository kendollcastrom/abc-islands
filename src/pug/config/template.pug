block config
	include utilis/vars
doctype html
html(lang='es' prefix='og: http://ogp.me/ns#', class='no-js')
	head
		include utilis/metatags
		include utilis/metaTagsPWA
		link(rel="preconnect", href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css")
		<link rel="preconnect" href="https://fonts.googleapis.com"><link rel="preconnect" href="https://fonts.gstatic.com" crossorigin><link href="https://fonts.googleapis.com/css2?family=Noto+Sans:ital,wght@0,100;0,300;0,400;0,500;0,700;1,600&display=swap" rel="stylesheet">
		<link href="https://fonts.cdnfonts.com/css/motion-control-neue-lite" rel="stylesheet">
		<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Rounded:opsz,wght,FILL,GRAD@48,400,0,0" />
		<link rel="stylesheet" href="https://fonts.googleapis.com/css2?family=Material+Symbols+Outlined:opsz,wght,FILL,GRAD@48,300,0,200" />
		link(rel="stylesheet" href="assets/css/styles.css")
		title= currentPage.slice(0,1).toUpperCase() + currentPage.slice(1) + " | " + siteName
	body(class=currentPage, itemscope itemtype="http://schema.org/WebPage")
		
		//-here start the Main Header
		header(class='main-header')
			include ../components/top-nav.html
			block main-header
		//-here ends the Main Header
		//-here start the Main Container
		main(class='main-container')
			block main-container
			//- div(class="menu__wrapper")
		//-here ends the Main Container
		//-here start the Main Footer//
		footer(class='main-footer')
			include ../components/cookies-component.html
			include ../components/scroll-button.html
			include ../components/menu-fixed.html
			include ../components/footer.html
			block main-footer
		//-here ends the Main Footer
		<script src="https://kit.fontawesome.com/de643c382f.js" crossorigin="anonymous"></script>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/js/bootstrap.bundle.min.js" integrity="sha384-MrcW6ZMFYlzcLA8Nl+NtUVF0sA7MsXsP1UyJoMp4YLEuNSfAP+JcXn/tWtIaxVXM" crossorigin="anonymous"></script>
		script(src="assets/js/scripts-min.js")
		script(src="assets/js/parlay-calculator.js")
		// Start of Tawk.to Script
		script(type='“text/javascript”').
			var Tawk_API=Tawk_API||{}, Tawk_LoadStart=new Date();
			(function(){
			var s1=document.createElement(“script”),s0=document.getElementsByTagName(“script”)[0];
			s1.async=true;
			s1.src=‘https://embed.tawk.to/5e39df78298c395d1ce638e6/default’;
			s1.charset=‘UTF-8’;
			s1.setAttribute(‘crossorigin’,‘*’);
			s0.parentNode.insertBefore(s1,s0);
			})();
		// End of Tawk.to Script
		block scripts
